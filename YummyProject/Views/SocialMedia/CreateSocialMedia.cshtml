@model YummyProject.Models.SocialMedia
@{
    ViewBag.Title = "CreateSocialMedia";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Sosyal Medya Ekle</h2>


<form method="post">

    <label>Url</label>
    @Html.TextAreaFor(a => a.Url, new { @class = "form-control" })
    <br />
    <label>Başlık</label>
    @Html.TextAreaFor(a => a.Title, new { @class = "form-control" })
    <br />
    <label>İkon</label>
    @Html.TextAreaFor(a => a.Icon, new { @class = "form-control" })
    <br />

    <button class="btn btn-primary">Kaydet</button>

</form>
